<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../app/code/Magento/Webapi/etc/webapi.xsd">
     <!-- GET RMA -->
    <route url="/V1/rma/:from/:to" method="GET">
        <service class="FutureActivities\Api\Api\RmaInterface" method="getRmaList"/>
        <resources>
          <resource ref="anonymous"/>
        </resources>       
    </route>
    <route url="/V1/rma/:idRma" method="POST">
        <service class="FutureActivities\Api\Api\RmaInterface" method="modifyRma"/>
        <resources>
          <resource ref="anonymous"/>
        </resources>       
    </route>

</routes>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="test">
                <settings>
                    <confirm>
                        <message translate="true">This could take time if you choose many products</message>
                        <title translate="true"></title>
                    </confirm>
                    <url path="backendurlrewrite/product/massGenerate"/>
                    <type>generate</type>
                    <label translate="true">Generate URL Rewrites</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>

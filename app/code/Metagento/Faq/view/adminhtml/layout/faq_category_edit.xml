<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="editor"/>
    <body>
        <referenceBlock name="menu">
            <action method="setActive">
                <argument name="itemId" xsi:type="string">Metagento_Faq::category</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Metagento\Faq\Block\Adminhtml\Category\Edit" name="faq_category_edit"></block>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Metagento\Faq\Block\Adminhtml\Category\Edit\Tabs" name="faq_category_edit_tabs"/>
        </referenceContainer>
    </body>
</page>

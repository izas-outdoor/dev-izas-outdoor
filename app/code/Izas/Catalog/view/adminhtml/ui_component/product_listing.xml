<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="export_images">
                <settings>
                    <confirm>
                        <message translate="true">Export Images?</message>
                        <title translate="true"></title>
                    </confirm>
                    <url path="izas_catalog/gallery/export"/>
                    <type>export_images</type>
                    <label translate="true">Export Images</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>

			<schedule>0 5 * * *</schedule>
		</job>
		<job instance="Izas\Empathy\Cron\FeedFr" method="execute" name="izas_empathy_feed_cron_fr">
			<schedule>0 4 * * *</schedule>
		</job>
		<job instance="Izas\Empathy\Cron\FeedIt" method="execute" name="izas_empathy_feed_cron_it">
			<schedule>0 3 * * *</schedule>
		</job>
		<job instance="Izas\Empathy\Cron\FeedCl" method="execute" name="izas_empathy_feed_cron_cl">
			<schedule>0 2 * * *</schedule>
		</job>
	</group>
</config>
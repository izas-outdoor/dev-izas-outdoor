<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
	
		<template id="quick_orde_verinet_details" name="Programing Order">
			<div>
				<table class="table">
					<tr>
						<td>
						   #NO
						</td>
						 <t t-foreach="attribute.value_ids" t-as="v">
							 <td class="att_value"><t t-esc="v.name"/></td>
						</t>
					</tr>
					<tr>
						<td class="att-img-td">
							<span t-field="product_tmp_id.image_1920" t-options="{'widget': 'image', 'class': 'img-rounded img-rounded o_image_64_max'}" />
						</td>
						<t t-foreach="attribute.value_ids" t-as="line_id">
							<t t-set="onhand" t-value="get_onhand_quntity(product_tmp_id,line_id)"/>
							<td id="templte-line" t-att-line_id="line_id.id" t-att-product_id="product_tmp_id.id" t-att-quick_order="quick_order.id" t-attr-onhand="onhand">
								<span t-if="quick_order.type == 'replenish'" class="td-on-hand-att" t-esc="onhand"/>
								<input type="text" name="value" placeholder="0" t-att-value="get_current_quantity(product_tmp_id,line_id)" t-att-disabled="check_availability(product_tmp_id,line_id)" class="input-value-class"/>
							</td>
						</t>
					</tr>
				</table>
			</div>
		</template>
	</data>
</odoo>


